<form class="inline" method="post" action="{{ path('app_habitat_delete', {'id': habitat.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet habitat?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ habitat.id) }}">
    <button>
        {{ include('_partials/icons/_trash.html.twig') }}
    </button>
</form>
